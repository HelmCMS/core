{
    "name": "helmcms/core",
    "description": "Core files for Helm CMS",
    "authors": [
        {
            "name": "Adam Patterson",
            "email": "hello@adampatterson.ca"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.3.*",
        "michelf/php-markdown": "1.4.*",
        "intervention/image": "2.*",
        "cviebrock/eloquent-sluggable": "2.*",
        "monolog/monolog": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*@dev",
        "orchestra/testbench": "2.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/controllers/Api"
        ],
        "psr-4": {
            "Helm\\Core\\": "src/Helm/Core/"
        }
    },
    "minimum-stability": "stable"
}
